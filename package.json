{
	"name": "sharp-icons",
	"displayName": "Sharp Icons",
	"version": "1.1.7",
	"preview": false,
	"publisher": "CiberTurtle",
	"description": "Sharp looking icons for vscode",
	"icon": "images/icon.png",
	"repository": "https://github.com/CiberTurtle/Sharp-Icons",
	"bugs": "https://github.com/CiberTurtle/Sharp-Icons/issues",
	"homepage": "https://github.com/CiberTurtle/Sharp-Icons",
	"license": "MIT",
	"keywords": [
		"Icons",
		"Sharp",
		"Crisp"
	],
	"categories": [
		"Themes"
	],
	"galleryBanner": {
		"color": "#42A5F5",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.63.0"
	},
	"contributes": {
		"iconThemes": [
			{
				"id": "sharp-icons",
				"label": "Sharp Icons",
				"path": "./icon-theme.json"
			}
		]
	},
	"scripts": {
		"start": "nodemon --watch src src/index.js",
		"build": "node src/index.js && mkdir -p build",
		"package": "vsce package -o ./build/sharp-icons.vsix",
		"prepublishOnly": "npm run build && vsce publish"
	},
	"devDependencies": {
		"nodemon": "^2.0.15",
		"vsce": "^2.7.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
